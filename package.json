{
  "name": "deeply-copy",
  "version": "1.0.0",
  "description": "custom deep copy function to make a brand new immutable object",
  "main": "src/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./build/types/*.d.cts",
        "default": "./build/cjs/*.cjs"
      },
      "import": {
        "types": "./build/types/*.d.ts",
        "default": "./build/esm/*.js"
      }
    }
  },
  "type": "module",
  "scripts": {
    "test": "jest",
    "build:esm": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "eslint": "npx eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f-lab-edu/deeply-copy.git"
  },
  "keywords": [
    "deepCopy",
    "deepClone"
  ],
  "author": "Kangsik Kevin Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/f-lab-edu/deeply-copy/issues"
  },
  "homepage": "https://github.com/f-lab-edu/deeply-copy#readme",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.5.0",
    "@jest/globals": "^29.7.0",
    "@types/eslint__js": "^8.42.3",
    "babel-jest": "^29.7.0",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.3.2",
    "ts-jest": "^29.1.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1"
  }
}
